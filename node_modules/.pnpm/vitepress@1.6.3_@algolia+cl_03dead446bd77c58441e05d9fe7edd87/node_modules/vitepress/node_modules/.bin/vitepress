#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_03dead446bd77c58441e05d9fe7edd87/node_modules/vitepress/bin/node_modules:/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_03dead446bd77c58441e05d9fe7edd87/node_modules/vitepress/node_modules:/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_03dead446bd77c58441e05d9fe7edd87/node_modules:/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_03dead446bd77c58441e05d9fe7edd87/node_modules/vitepress/bin/node_modules:/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_03dead446bd77c58441e05d9fe7edd87/node_modules/vitepress/node_modules:/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_03dead446bd77c58441e05d9fe7edd87/node_modules:/mnt/c/Users/augus/Documents/Programação/HydraGuide/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
